{
  "name": "@vibekit/cli",
  "version": "0.1.3",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "prebuild": "bun run scripts/bundle-skills.ts",
    "prebuild:skills": "bun run scripts/bundle-skills.ts",
    "build": "bun build src/index.ts --compile --outfile bin/vibekit",
    "build:linux-x64": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile bin/vibekit-linux-x64",
    "build:darwin-arm64": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile bin/vibekit-darwin-arm64",
    "build:all": "bun run prebuild && bun run build:linux-x64 && bun run build:darwin-arm64",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf bin dist"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "@vibekit/config": "workspace:*",
    "@vibekit/db": "workspace:*",
    "@vibekit/keyring": "workspace:*",
    "@vibekit/mcp-server": "workspace:*",
    "@vibekit/provider-interface": "workspace:*",
    "@vibekit/provider-keyring": "workspace:*",
    "@vibekit/provider-vault": "workspace:*",
    "algosdk": "^3.5.2",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
